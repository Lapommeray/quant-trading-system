# QuantConnect Trading System Environment Variables

# API Keys
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_API_SECRET=your_alpaca_api_secret_here
QUANTCONNECT_API_KEY=your_quantconnect_api_key_here
QUANTCONNECT_API_SECRET=your_quantconnect_api_secret_here

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/qts_db
REDIS_URL=redis://localhost:6379

# Trading Configuration
TRADING_MODE=paper
RISK_MANAGEMENT_ENABLED=true
NEVER_LOSS_PROTECTION=true
MAX_POSITION_SIZE=0.1
STOP_LOSS_PERCENTAGE=0.02

# Quantum System Configuration
QUANTUM_ENABLED=true
QUANTUM_TREE_HEIGHT=10
ENCRYPTION_ENABLED=true

# Monitoring and Logging
LOG_LEVEL=INFO
PROMETHEUS_ENABLED=true
GRAFANA_ENABLED=true
MONITORING_PORT=9090

# Environment
QTS_ENV=development
DEBUG=false
